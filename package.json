{
  "name": "harald",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "tinacms dev -c 'astro dev' --datalayer-port 3001",
    "start": "astro dev",
    "build": "pnpm tinacms build && astro build",
    "build:local": "pnpm tinacms build --datalayer-port 3001 && astro build",
    "preview": "astro preview",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "astro": "astro",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.0.3",
    "@tinacms/cli": "^1.5.39",
    "astro": "^4.0.5",
    "fld-grd": "^1.2.0",
    "tailwindcss": "^3.4.0",
    "tinacms": "^1.5.28"
  },
  "volta": {
    "node": "18.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2"
  },
  "lint-staged": {
    "format": "prettier --write . --plugin=prettier-plugin-astro"
  }
}
